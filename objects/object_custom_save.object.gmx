            <string>
    if( !object_custom_setting.camera_move ) { if( show_question( "저장 하시겠습니까?" ) ) {
        
        var setting = object_custom_setting;
    
        if( setting.item_ball &amp;&amp; setting.item_pipe &amp;&amp; !( setting.item_pipe_in ^^ setting.item_pipe_out ) ) {
        
            ini_open( "custom_stages.ini" );
            if( ini_section_exists( "stage" + string( save_slot ) ) ) { ini_section_delete( "stage" + string( save_slot ) ); }
            
            ini_write_real( "stage" + string( save_slot ), "stage size", setting.custom_size );
            ini_write_real( "stage" + string( save_slot ), "grid width", setting.grid_width );
            ini_write_real( "stage" + string( save_slot ), "grid height", setting.grid_height );
            for( var i = 0; i &lt; setting.grid_width; i++ ) { for( var j = 0; j &lt; setting.grid_height; j++ ) { ini_write_real( "stage" + string( save_slot ), "grid block [" + string( i ) + "," + string( j ) + "]", setting.grid[i,j] ); } }
            
            ini_close();
            
            setting.saved = true;
            
            show_message( "저장을 완료 했습니다" );
        
        } else {
        
            if( !setting.item_ball ) { show_message( "에러!#공이 없습니다" ); }
            if( !setting.item_pipe ) { show_message( "에러!#파이프가 없습니다" ); }
            if( item_pipe_in ^^ item_pipe_out ) {
            
                if( !setting.item_pipe_in ) { show_message( "에러!#입구 파이프가 없습니다" ); }
                if( !setting.item_pipe_out ) { show_message( "에러!#출구 파이프가 없습니다" ); }
            
            }
        
        }
        
    } }
</string>
